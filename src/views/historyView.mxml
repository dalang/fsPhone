<?xml version="1.0" encoding="utf-8"?>
<s:View xmlns:fx="http://ns.adobe.com/mxml/2009" 
		xmlns:s="library://ns.adobe.com/flex/spark" title="historyView">
	<fx:Script>
		<![CDATA[
			import mx.collections.ArrayList;
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<s:navigationContent/>
	
	<s:titleContent>
		<s:TextInput id="key" width="100%"/>
	</s:titleContent>
	
	<s:actionContent>
		<s:Button icon="@Embed('assets/search.png')"/>
	</s:actionContent>
	
	<s:List id="list" top="0" bottom="0" left="0" right="0" 
			dataProvider="{parentDocument.dialedCalls}" >
		<s:itemRenderer>
			<fx:Component>
				<s:IconItemRenderer label="{parentDocument.dialedCalls.name}" messageField="time">
				</s:IconItemRenderer>
			</fx:Component>
		</s:itemRenderer>
	</s:List>
</s:View>
